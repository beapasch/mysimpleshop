<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog confirm="onSortConfirmed" sortDescending="true" >
		<!--Sort Rating -->
		<sortItems>
			<ViewSettingsItem text="{/#Review/ChangedAt/@sap:label}"
				key="ChangedAt" selected="true" />
			<ViewSettingsItem text="{/#Review/Rating/@sap:label}"
				key="Rating" />
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>